{
  "name": "solana-liquidity-agent",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "description": "AI-powered Raydium liquidity pool analyzer with Helius integration",
  "scripts": {
    "clean": "rm -rf api dist node_modules/.cache",
    "prebuild": "bun run clean",
    "dev": "bun run --watch src/index.ts",
    "worker": "bun run --watch src/worker.ts",
    "start": "bun run src/index.ts",
    "start:api": "bun run src/index.ts",
    "start:worker": "bun run src/worker.ts",
    "build": "cp -r src api/ || xcopy /E /I src api\\src",
    "vercel-build": "npm run build",
    "test:helius": "bun run scripts/test-helius.ts",
    "test:redis": "bun run scripts/test-redis.ts",
    "test:worker": "bun run src/worker.ts",
    "test:full": "bun run scripts/test-full-flow.ts",
    "test:raydium": "bun run scripts/test-raydium-parser.ts",
    "test:transactions": "bun run scripts/test-transaction-parser.ts",
    "test:price": "bun run scripts/test-price-tvl.ts",
    "test:active": "bun run scripts/test-active-pool.ts",
    "test:hybrid": "bun run scripts/test-hybrid-pools.ts",
    "test:mint": "bun run scripts/test-mint-input.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@lucid-agents/core": "latest",
    "@lucid-agents/hono": "latest",
    "@lucid-agents/http": "latest",
    "@lucid-agents/payments": "latest",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.58",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.95.0",
    "@supabase/supabase-js": "^2.39.0",
    "bn.js": "^5.2.2",
    "bullmq": "^5.0.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.3.1",
    "helius-sdk": "^1.3.3",
    "ioredis": "^5.3.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "bun-types": "^1.3.0",
    "typescript": "^5.9.2"
  }
}